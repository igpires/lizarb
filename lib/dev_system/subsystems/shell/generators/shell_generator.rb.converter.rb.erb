# view converter.rb.erb

  def self.call(menv)
    ret = true
    super
    
    menv[:convert_out] = <%= arg_name.camelize %>.convert menv[:convert_in]
  rescue => e
    raise if menv[:raise_errors]
    log stick :light_white, :red, :b, "#{e.class}: #{e.message}"
    menv[:error] = e
    menv[:convert_out] = menv[:convert_in]
  ensure
    nil
  end
